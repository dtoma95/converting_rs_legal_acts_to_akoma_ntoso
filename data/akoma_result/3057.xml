									<item id="clan2-stav1-tac4-podtac14">
										<num>(14)</num>
										<content>
											<p> подводни спорт,</p>
										</content>
									</item>
									<item id="clan2-stav1-tac4-podtac15">
										<num>(15)</num>
										<content>
											<p> рафтинг,</p>
										</content>
									</item>
									<item id="clan2-stav1-tac4-podtac16">
										<num>(16)</num>
										<content>
											<p> рагби,</p>
										</content>
									</item>
									<item id="clan2-stav1-tac4-podtac17">
										<num>(17)</num>
										<content>
											<p> рагби 13,</p>
										</content>
									</item>
									<item id="clan2-stav1-tac4-podtac18">
										<num>(18)</num>
										<content>
											<p> самбо,</p>
										</content>
									</item>
									<item id="clan2-stav1-tac4-podtac19">
										<num>(19)</num>
										<content>
											<p> скокови у воду,</p>
										</content>
									</item>
									<item id="clan2-stav1-tac4-podtac20">
										<num>(20)</num>
										<content>
											<p> скијање на води,</p>
										</content>
									</item>
									<item id="clan2-stav1-tac4-podtac21">
										<num>(21)</num>
										<content>
											<p> спортско пењање,</p>
										</content>
									</item>
									<item id="clan2-stav1-tac4-podtac22">
										<num>(22)</num>
										<content>
											<p> хокеј на трави,</p>
										</content>
